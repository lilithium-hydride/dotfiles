* {
	border: none;
	border-radius: 0;
	/*font-family: BitstreamVeraSansMono Nerd Font;*/
	/*font-family: SFMono Nerd Font Mono;*/
	/*font-family: SauceCodePro Nerd Font;*/
	font-family: SauceCodePro Nerd Font;
	font-size: 12px;
	min-height: 16px;
}

window#waybar {
	background: transparent;
}

window#waybar.hidden {
	opacity: 0.2;
}

#window {
	margin-top: 10px;
	padding-left: 16px;
	padding-right: 16px;
	border-radius: 26px;
	/*transition: none;*/
	/*
	color: #f8f8f2;
	background: #282a36;
	*/
	color: transparent;
	background: transparent;
}

window#waybar.termite #window,
window#waybar.Firefox #window,
window#waybar.Librewolf #window,
window#waybar.Navigator #window,
window#waybar.PCSX2 #window {
	color: #4d4d4d;
	background: #e6e6e6;
}

#workspaces {
	margin-top: 10px;
	margin-left: 12px;
	margin-bottom: 0;
	border-radius: 26px;
	/*background: #282a36;*/
	background: #191724;
	/*transition: width 2s;*/
}

#workspaces button {
	transition: none;
	/*color: #f8f8f2;*/
	color: #e0def4;
	background: transparent;
	font-size: 16px;
}

#workspaces button.focused {
	/*color: #9aedfe;*/
	color: #9ccfd8;
}

#workspaces button:hover {
	transition: none;
	box-shadow: inherit;
	text-shadow: inherit;
	/*color: #ff79c6;*/
	color: #c4a7e7;
}

#custom-media {
	margin-top: 10px;
	margin-left: 8px;
	padding-left: 16px;
	padding-right: 16px;
	padding-top: 4px;
	margin-bottom: 0;
	border-radius: 26px;
	background: #282a36;
	transition: none;
	color: #4d4d4d;
	/*background: #5af78e;*/
	background: #7fb59f;
}

#mpd.disconnected,
#mpd.stopped {
	color: #f8f8f2;
	background: #282a36;
}

